#include "setup.h"
#include "main.h"

typedef struct {                                // object data type
  uint8_t Buf[32];
  uint8_t Idx;
} MSGQ_obj;
osMessageQueueId_t Q_name;

int Init_MsgQueue () {
  osMessageQueueNew(MSGQ_obj, sizeof(MSGQ_obj), NULL);
  if (Q_name == NULL) {
	  return -1;
  }
  return 0;
}
