#include "setup.h"

typedef struct {                                // object data type
  uint8_t Buf[32];
  uint8_t Idx;
} MSGQ_obj;

int Init_MsgQueue (osMessageQueueId_t Q_name) {


  Q_name = osMessageQueueNew(MSGQ_obj, sizeof(MSGQ_obj), NULL);
  if (Q_name == NULL) {
	  return -1;
  }
  return 0;
}
