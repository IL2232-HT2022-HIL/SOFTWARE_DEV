#include "setup.h"
#include "main.h"

typedef struct {                                // object data type
  uint8_t Buf[32];
  uint8_t Idx;
} MSGQUEUE_OBJ_t;
osMessageQueueId_t mid_MsgQueue;

int Init_MsgQueue () {
	mid_MsgQueue = osMessageQueueNew(MSGQUEUE_OBJECTS, sizeof(MSGQUEUE_OBJ_t), NULL);
  if (Q_name == NULL) {
	  return -1;
  }
  return 0;
}
