#include "cmsis_os.h"
#include "setup.h"

int gateway_receive_BUF[10];
int pos = 0;

int* HiL_gateway_get_message(){
	MSGQ_obj = msg;
	status = osMessageQueueGet(UART_Q, &msg, NULL, 10U);
	if (status == osOK){
		pos = (pos +1)%10;
		gateway_receive_BUF[pos] = msg;
		return gateway_receive_BUF[pos];
	}
	else{
		return NULL;
	}
}

int HiL_gateway_send_message(int* msg){

}
